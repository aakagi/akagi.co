{
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "eslint.format.enable": true,
  "eslint.alwaysShowStatus": true,
  "eslint.validate": ["javascript", "javascriptreact"],
  "eslint.run": "onSave",
  "explorer.confirmDelete": false,
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true
  }
}
